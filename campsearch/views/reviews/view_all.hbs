<div class="container-fluid">
        <div class="row row-cols-8 justify-content-center">
            <div class="col-8 col-md-5 btn-group-vertical" role="group">
                {{#each reviewList}}
                    <a class="mt-3 btn btn-lg btn-block btn-outline-dark"
                       href="/reviews/view?id={{ id }}">{{ title }}
                    <h6 class="mt-1">{{date}}</h6></a>
                {{/each}}
            </div>
        </div>
</div>

<img src="/assets/images/camping.jpg" class="img-fluid mt-4" alt="camping sunset">

